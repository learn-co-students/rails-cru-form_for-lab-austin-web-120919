<h1>Song Form</h1>
<%= form_for(@song) do |f| %>
    <%= f.label 'Name: '%>
    <%= f.text_field :name %>

    <%= f.label 'Artist: '%>
    <%= f.collection_select :artist_id, Artist.all,:id,:name %>

    <%= f.label 'Genre: '%>
    <%= f.collection_select :genre_id, Genre.all,:id,:name %>

    <%= f.submit%>
<% end %>